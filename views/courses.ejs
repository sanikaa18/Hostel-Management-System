<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Courses</title>
</head>
<body>
    <h1>Courses</h1>
    <ul>
        <% courses.forEach(function(course) { %>
            <li>
                Name: <%= course.course_name %>, 
                Description: <%= course.course_description %>
            </li>
        <% }); %>
    </ul>
</body>
</html>
